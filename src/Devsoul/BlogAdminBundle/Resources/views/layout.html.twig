{% extends "base.html.twig" %}
{% block stylesheets %}
{% endblock %}
{% block body %}
    <section id="page">
        {{ include('@DevsoulBlogAdmin/header.html.twig') }}
        {{ include('@DevsoulBlogAdmin/sidebar.html.twig') }}
        <div id="main-content">
            <div class="container">
                <section style="padding-top: 25px;">
                    {% for type, messages in app.session.flashBag.all() %}
                        {% for message in messages %}
                            <p class="session-message">
                                {{ message }}
                            </p>
                        {% endfor %}
                    {% endfor %}
                    {% block section %}
                    {% endblock %}
                </section>
            </div>
        </div>
    </section>
{% endblock %}
{% block javascripts %}
{% endblock %}